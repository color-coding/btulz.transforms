<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- 此模板的定义类型额外映射说明 -->
<ns2:DataTypeMappings xmlns:ns2="http://colorcoding.org/btulz/transformers">
    <!-- SubType未指定则忽略子项类型 -->
    <Mapping DataType="Alphanumeric" Mapped="String"/>
    <Mapping DataType="Memo" Mapped="String"/>
    <Mapping DataType="Numeric" Mapped="Integer"/>
    <Mapping DataType="Decimal" Mapped="BigDecimal"/>
    <Mapping DataType="Date" SubType="Default" Mapped="DateTime"/>
    <Mapping DataType="Date" SubType="Date" Mapped="DateTime"/>
    <Mapping DataType="Date" SubType="Time" Mapped="Short"/>
    <Mapping DataType="Bytes" Mapped="byte[]"/>
</ns2:DataTypeMappings>
