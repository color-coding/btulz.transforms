<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:CommandBuilder Description="生成代码" xmlns:ns2="http://colorcoding.org/btulz/shell/commands">
    <Item Content="java" Editable="false" />
    <Item Content="-jar btulz.transforms.core-0.1.0.jar" Description="使用jar包" Editable="false" />
    <Item Content="code" Description="子命令" Editable="false" />
    <Item Content="-TemplateFolder=${VALUE}" Description="使用的模板" >
        <ValidValues ClassName="org.colorcoding.tools.btulz.shell.commands.CodeTemplateGetter" />
    </Item>
    <Item Content="-OutputFolder=${VALUE}" Description="输出目录" Value="${WORK_FOLDER}temp" >
        <ValidValues ClassName="javax.swing.JFileChooser"/>
    </Item>
    <Item Content="-GroupId=${VALUE}" Description="组标记" Value="club.ibas" />
    <Item Content="-ArtifactId=${VALUE}" Description="项目标记" Value="ibas" />
    <Item Content="-ProjectVersion=${VALUE}" Description="项目标记" Value="0.0.1" />
    <Item Content="-ProjectUrl=${VALUE}" Description="项目地址" Value="http://ibas.club/" />
    <Item Content="-Domains=${VALUE}" Description="模型文件" Value="${WORK_FOLDER}domains" >
        <ValidValues ClassName="javax.swing.JFileChooser"/>
    </Item>
    <Item Content="-Parameters=[${VALUE}]" Description="其他参数">
        <Items>
            <Item Content="{&quot;name&quot;:&quot;Company&quot;,&quot;value&quot;:&quot;${VALUE}&quot;}" Description="公司标识" Value="CC" />
            <Item Content=",{&quot;name&quot;:&quot;ibasVersion&quot;,&quot;value&quot;:&quot;${VALUE}&quot;}" Description="ibas版本" Value="0.1.1" />
            <Item Content=",{&quot;name&quot;:&quot;jerseyVersion&quot;,&quot;value&quot;:&quot;${VALUE}&quot;}" Description="jersey版本" Value="2.22.1" />
        </Items>
    </Item>
    <Item Content="-Release" Description="释放资源" Optional="true" />
</ns2:CommandBuilder>